<?xml version="1.0" encoding="utf-8"?>
<androidx.constraintlayout.widget.ConstraintLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    app:layout_behavior="@string/appbar_scrolling_view_behavior"
    tools:context=".Empmenu"
    tools:showIn="@layout/empmenu">
    <RelativeLayout
        android:layout_width="match_parent"
        android:layout_height="match_parent"
        android:background="@drawable/cs_bg">
        <LinearLayout
            android:layout_width="match_parent"
            android:layout_height="match_parent"
            android:gravity="center"
            android:layout_marginTop="10dp"
            android:paddingLeft="25dp"
            android:paddingRight="25dp"
            android:orientation="vertical">

            <ImageView
                android:id="@+id/dayo"
                android:layout_width="200dp"
                android:layout_height="70dp"
                android:layout_marginTop="20dp"
                android:layout_gravity="center"

                app:srcCompat="@drawable/cs_off" />
            <ImageView
                android:id="@+id/subsign"
                android:layout_width="200dp"
                android:layout_height="70dp"
                android:layout_marginTop="20dp"
                android:layout_gravity="center"

                app:srcCompat="@drawable/subsign" />
            <ImageView
                android:id="@+id/signature"
                android:layout_width="200dp"
                android:layout_height="70dp"
                android:layout_marginTop="20dp"
                android:layout_gravity="center"

                app:srcCompat="@drawable/cs_sign" />

            <ImageView
                android:id="@+id/del"
                android:layout_width="200dp"
                android:layout_height="70dp"
                android:layout_marginTop="20dp"

                android:layout_gravity="center"
                app:srcCompat="@drawable/cs_cancel" />
            <ImageView
                android:id="@+id/search"
                android:layout_width="200dp"
                android:layout_height="70dp"
                android:layout_marginTop="20dp"
                android:layout_gravity="center"
                app:srcCompat="@drawable/cs_record" />
            <ImageView
                android:id="@+id/ret"
                android:layout_width="200dp"
                android:layout_height="70dp"
                android:layout_marginTop="20dp"

                android:layout_gravity="center"
                app:srcCompat="@drawable/cs_logout" />

        </LinearLayout>
    </RelativeLayout>
</androidx.constraintlayout.widget.ConstraintLayout>